# 服务器配置
PORT=3000
NODE_ENV=development

# PostgreSQL 配置
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=
DB_NAME=searchimg_db

# 火山引擎 Doubao API 配置
# 获取方式: https://console.volcengine.com/ark/region:ark+cn-beijing/apiKey
DOUBAO_API_KEY=your_api_key_here
DOUBAO_BASE_URL=https://ark.cn-beijing.volces.com/api/v3

# 文件上传配置
UPLOAD_DIR=uploads
MAX_FILE_SIZE=52428800

# Jina AI API 配置
JINA_API_KEY=your_api_key_here

# 阿里云 DashScope (Qwen) 配置
# 请在此处填入您的 DashScope API Key: https://dashscope.console.aliyun.com/apiKey
DASHSCOPE_API_KEY=your_api_key_here

# 火山引擎 TOS 配置
# 请在此处填入您的 TOS 配置
TOS_REGION=cn-beijing
TOS_ACCESS_KEY=your_api_key_here
TOS_SECRET_KEY=your_api_key_here
TOS_BUCKET=your_bucket_here

# CORS 配置
CORS_ORIGIN=http://localhost:5173

